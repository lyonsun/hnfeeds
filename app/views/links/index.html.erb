
    <div class="list-group">
      <% @links.each do |link| %>
      <div class="list-group-item">
        <h4><%= link_to link.title, link %></h4>
        <small class="text-muted">Submitted <%= time_ago_in_words(link.created_at) %> by <%= link.user.email %></small>
      </div>
      <% end %>
    </div>